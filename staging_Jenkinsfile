def runTest(){
  println("doing some testing")
}

def build(){

}

def deploy(env){
  println("deploy to ${env}")
}

node {
  checkout scm
  switch(env.BRANCH_NAME) {  
    case "develop":
      build()
      runTest()
      deploy("staging")
    break

    default:
      build()
      runTest()
    break
  }
}